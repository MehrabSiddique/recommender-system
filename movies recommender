;\n","    }\n","  "]},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<IPython.core.display.Javascript object>"],"application/javascript":["download(\"download_90eb4848-fed7-456e-b2de-59484227921e\", \"movies.pkl\", 2276873)"]},"metadata":{}}]},{"cell_type":"code","source":["from google.colab import files\n","files.download('movies.pkl')"],"metadata":{"id":"T4IyKCdVvfDi"},"execution_count":null,"outputs":[]}]}